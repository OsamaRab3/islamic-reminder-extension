{
    "manifest_version": 3,
    "name": "Islamic Reminder",
    "version": "1.0",
    "description": "Sends Islamic reminder notifications every 5 minutes.",
    "permissions": ["notifications", "alarms"],
    "background": {
      "service_worker": "background.js"
  },

    "icons": {
      "128": "image.png"
    },
    "action": {
      "default_title": "Click to enable reminders"
    }
  }
  